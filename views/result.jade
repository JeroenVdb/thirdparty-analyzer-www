html
  head
    title!= title
  body
    h1!= message
    for provider in result
        ul
            li #{provider.name}
                ul
                    li requests: #{provider.totals.requests}
                        ul
                            for entry in provider.entries
                                li #{entry.request.url}
                    li timings: #{provider.totals.timings}
                    li size: #{provider.totals.content.size}
