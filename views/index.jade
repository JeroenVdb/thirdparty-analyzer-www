html
  head
    title!= title
  body
    h1!= message
    form(action="/formHandler", method="post", enctype="multipart/form-data")
        label First party urls, one url per line
        br
        textarea(name="firstparty", rows="6", cols="100")
        br
        br
        label HAR File
        br
        input(type="file", name="har")
        br
        br
        br
        button(type="submit")= submit
    form(action="/wptFormHandler", method="post", enctype="multipart/form-data")
        label Url you want to send to WPT
        br
        input(type="text", name="url")
        br
        button(type="submit")= submit
